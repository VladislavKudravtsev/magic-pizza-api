{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "eol-last": "off",
        "radix": "off",
        "no-underscore-dangle": "off",
        "no-bitwise": "off",
        "no-unused-vars": "off"
    }
}